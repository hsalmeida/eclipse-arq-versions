<team-customization xmlns="http://com.ibm.team.process">
    <role-definitions>
        <role-definition cardinality="single" role-id="role-id"/>
    </role-definitions>
    <iteration id="iteration_id">
        <permissions>
            <role id="role_id">
                <operation id="operation_id">
                    <action id="any"/>
                </operation>
            </role>
        </permissions>
        <behavior>
            <role id="role_id">
                <operation id="operation_id">
                    <preconditions>
                        <precondition description="description" id="condition_id" name="display_name" overrulable="true"/>
                    </preconditions>
                    <followup-actions>
                        <followup-action id="action_id" name="name"/>
                    </followup-actions>
                </operation>
            </role>
            <change-event id="event_id">
                <followup-actions>
                    <followup-action id="action_id" name="name"/>
                </followup-actions>
            </change-event>
        </behavior>
    </iteration>
</team-customization>
